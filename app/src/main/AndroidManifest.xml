<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dev.nick.accessoriestest">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="dev.nick.accessoriestest.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="dev.nick.accessoriestest.TestsList">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="dev.nick.accessoriestest.FileImageTest"
            android:label="FileImageTest" />

        <activity
            android:name="dev.nick.accessoriestest.DrawableImageTest"
            android:label="DrawableImageTest" />

        <activity
            android:name="dev.nick.accessoriestest.VectorImageTest"
            android:label="VectorImageTest" />

        <activity
            android:name="dev.nick.accessoriestest.MipMapImageTest"
            android:label="MipMapImageTest" />

        <activity
            android:name="dev.nick.accessoriestest.ContentImageTest"
            android:label="ContentImageTest" />

        <activity
            android:name="dev.nick.accessoriestest.AssetsImageTest"
            android:label="AssetsImageTest" />

        <activity
            android:name="dev.nick.accessoriestest.NetworkImageTest"
            android:label="NetworkImageTest" />

        <activity
            android:name="dev.nick.accessoriestest.LoadImageTest"
            android:label="LoadImageTest" />

        <activity
            android:name="dev.nick.accessoriestest.SyncLoadTest"
            android:label="SyncLoadTest" />

        <activity
            android:name="dev.nick.accessoriestest.AssetGifTest"
            android:label="AssetMovieTest" />

        <activity
            android:name="dev.nick.accessoriestest.AssetVideoTest"
            android:label="AssetVideoTest" />

        <activity
            android:name="dev.nick.accessoriestest.NetworkMovieTest"
            android:label="NetworkMovieTest" />

        <activity
            android:name="dev.nick.accessoriestest.Usage"
            android:label="Usage" />

        <activity
            android:name="dev.nick.accessoriestest.ScrollStateTest"
            android:label="ScrollStateTest" />

        <activity
            android:name="dev.nick.accessoriestest.CustomAnimationTest"
            android:label="CustomAnimationTest" />

        <activity
            android:name="dev.nick.accessoriestest.DownloadMachine"
            android:label="DownloadMachine" />
    </application>

</manifest>
